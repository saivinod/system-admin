#To start the virtual machine 
sudo virsh start <server_name>

#To make the static ip address 
network 192.168.0.0
broadcast 192.168.0.255
geteway 192.168.0.101

#Creating new user vinod
sudo useradd -m -s /bin/bash vinod
#giving sudo access to the user vinod
sudo usermod -a -G sudo

#copy the ssh-key
ssh-copy-id <username>@<host>

